require "bingo.rb"
require "pp"
bingo = Bingo.new

while bingo.check_rust?
  bingo.select_number
  print bingo.current_number
  print "\n"
end
print Bingo::MESSAGE_GAME_IS_OVER

bingo = Bingo.new
35.times{|n| bingo.select_number}

pp bingo.rust_numbers
pp bingo.used_numbers
pp bingo.current_number

print bicheck_unused? bingo.used_numbers










